{
  "build": {
    "arduino":{
      "ldscript": "nrf52833_xxaa.ld"
    },
    "core": "nRF5",
    "cpu": "cortex-m4",
    "extra_flags": "-DARDUINO_BBC_MICROBIT_V2 -DNRF52833_XXAA",
    "f_cpu": "64000000L",
    "mcu": "nrf52833",
    "variant": "BBCmicrobitV2",
    "zephyr": {
      "variant": "bbc_microbit_v2"
    }
  },
  "connectivity": [
    "bluetooth"
  ],
  "debug": {
    "onboard_tools": [
      "cmsis-dap"
    ],
    "svd_path": "nrf52.svd",
    "openocd_extra_args": [
      "-c",
      "transport select swd;"
    ],
    "jlink_device": "nRF52833_xxAA"
  },
  "frameworks": [
    "arduino",
    "zephyr"
  ],
  "name": "BBC micro:bit V2",
  "upload": {
    "maximum_ram_size": 131072,
    "maximum_size": 524288,
    "protocol": "cmsis-dap",
    "protocols": [
      "jlink",
      "nrfjprog",
      "stlink",
      "blackmagic",
      "cmsis-dap",
      "mbed"
    ]
  },
  "url": "https://microbit.org/new-microbit/",
  "vendor": "BBC"
}
